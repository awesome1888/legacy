{
  "presets": [
    "@babel/react",
    [
      "@babel/env",
      {
        "modules": "commonjs",
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      { "legacy": true }
    ],
    "@babel/plugin-proposal-object-rest-spread",
    [
      "@babel/plugin-proposal-class-properties",
      { "loose": true }
    ]
  ]
}
