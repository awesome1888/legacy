{
    "name": "app.admin",
    "version": "1.0.0",
    "description": "## Development startup",
    "scripts": {
        "start": "npm-run-all --parallel build.*.dev",
        "build": "npm-run-all --parallel build.*.prod",
        "build.server.prod": "cd server/; webpack --config webpack.server.js --mode production",
        "build.client.prod": "cd client/; webpack --config webpack.client.js --mode production",
        "build.dev": "npm-run-all --parallel build.*.dev",
        "build.server.dev": "cd server/; webpack --config webpack.js --mode development --watch;",
        "build.client.dev": "cd client/; webpack --config webpack.js --mode development --watch;"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "webpack-cli": "^3.3.0"
    }
}
